{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "My.App.ConsoleTest",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/My.App.ConsoleTest/My.App.ConsoleTest.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "My.App.Job",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/My.App.Job/My.App.Job.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/My.App.Job/My.App.Job.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/My.App.Job/My.App.Job.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}